#, no-c-format
msgid ""
"<varname>[rast2]</varname> - Pixel value of the pixel of interest from "
"<varname>rast2</varname>"
msgstr ""
"<varname>[rast2]</varname> - Valor do pixel de interesse do <varname>rast2</"
"varname>"

#. Tag: para
#: reference_raster.xml:7848
#, no-c-format
msgid ""
"<varname>[rast2.val]</varname> - Pixel value of the pixel of interest from "
"<varname>rast2</varname>"
msgstr ""
"<varname>[rast2.val]</varname> - Valor do pixel de interesse do "
"<varname>rast2</varname>"

#. Tag: para
#: reference_raster.xml:7851
#, no-c-format
msgid ""
"<varname>[rast2.x]</varname> - 1-based pixel column of the pixel of interest "
"from <varname>rast2</varname>"
msgstr ""
"<varname>[rast2.x]</varname> - coluna pixel 1-based do pixel de interesse "
"do<varname>rast2</varname>"

#. Tag: para
#: reference_raster.xml:7854
#, no-c-format
msgid ""
"<varname>[rast2.y]</varname> - 1-based pixel row of the pixel of interest "
"from <varname>rast2</varname>"
msgstr ""
"<varname>[rast2.y]</varname> - linha pixel 1-based do pixel de interesse "
"do<varname>rast2</varname>"

#. Tag: title
