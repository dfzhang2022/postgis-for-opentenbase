"<varname>rast1</varname>"
msgstr ""
"<varname>[rast1]</varname> - Valeur du pixel d'intérêt sur <varname>rast1</"
"varname>"

#. Tag: para
#: reference_raster.xml:7847
#, no-c-format
msgid ""
"<varname>[rast1.val]</varname> - Pixel value of the pixel of interest from "
"<varname>rast1</varname>"
msgstr ""
"<varname>[rast1.val]</varname> - Valeur du pixel d'intérêt de "
"<varname>rast1</varname>"

#. Tag: para
#: reference_raster.xml:7850
#, no-c-format
msgid ""
"<varname>[rast1.x]</varname> - 1-based pixel column of the pixel of interest "
"from <varname>rast1</varname>"
msgstr ""
"<varname>[rast1.x]</varname> - Colonne du pixel d'intérêt de <varname>rast1</"
"varname> (démarrant à 1)"

#. Tag: para
#: reference_raster.xml:7853
#, no-c-format
msgid ""
"<varname>[rast1.y]</varname> - 1-based pixel row of the pixel of interest "
"from <varname>rast1</varname>"
msgstr ""
"<varname>[rast1.y]</varname> - Ligne du pixel d'intérêt de <varname>rast1</"
"varname> (démarrant à 1)"

#. Tag: para
#: reference_raster.xml:7856
#, no-c-format
msgid ""
"<varname>[rast2]</varname> - Pixel value of the pixel of interest from "
"<varname>rast2</varname>"
msgstr ""
"<varname>[rast2]</varname> - Valeur du pixel d'intérêt sur <varname>rast2</"
"varname>"

#. Tag: para
#: reference_raster.xml:7859
#, no-c-format
msgid ""
"<varname>[rast2.val]</varname> - Pixel value of the pixel of interest from "
"<varname>rast2</varname>"
msgstr ""
"<varname>[rast2.val]</varname> - Valeur du pixel d'intérêt de "
"<varname>rast2</varname>"

#. Tag: para
#: reference_raster.xml:7862
#, no-c-format
msgid ""
"<varname>[rast2.x]</varname> - 1-based pixel column of the pixel of interest "
"from <varname>rast2</varname>"
msgstr ""
"<varname>[rast2.x]</varname> - Colonne du pixel d'intérêt de <varname>rast2</"
"varname> (démarrant à 1)"

#. Tag: para
#: reference_raster.xml:7865
#, no-c-format
msgid ""
"<varname>[rast2.y]</varname> - 1-based pixel row of the pixel of interest "
"from <varname>rast2</varname>"
msgstr ""
"<varname>[rast2.y]</varname> - Ligne du pixel d'intérêt de <varname>rast2</"
"varname> (démarrant à 1)"

#. Tag: title
#: reference_raster.xml:7873
#, no-c-format
msgid "Examples: Variants 1 and 2"
msgstr "Exemples : Variantes 1 et 2"

#. Tag: programlisting
#: reference_raster.xml:7875
#, no-c-format
msgid ""
"WITH foo AS (\n"
"    SELECT ST_AddBand(ST_MakeEmptyRaster(10, 10, 0, 0, 1, 1, 0, 0, 0), "
"'32BF'::text, 1, -1) AS rast\n"
")\n"
"SELECT\n"
"    ST_MapAlgebra(rast, 1, NULL, 'ceil([rast]*[rast.x]/[rast.y]+[rast."
"val])')\n"
"FROM foo;"
msgstr ""
"WITH foo AS (\n"
"    SELECT ST_AddBand(ST_MakeEmptyRaster(10, 10, 0, 0, 1, 1, 0, 0, 0), "
"'32BF'::text, 1, -1) AS rast\n"
")\n"
"SELECT\n"
"    ST_MapAlgebra(rast, 1, NULL, 'ceil([rast]*[rast.x]/[rast.y]+[rast."
"val])')\n"
"FROM foo;"

#. Tag: title
#: reference_raster.xml:7879
#, no-c-format
msgid "Examples: Variant 3 and 4"
msgstr "Exemples : Variantes 3 et 4"

#. Tag: programlisting
#: reference_raster.xml:7881
#, no-c-format
msgid ""
"WITH foo AS (\n"
"    SELECT 1 AS rid, ST_AddBand(ST_AddBand(ST_AddBand(ST_MakeEmptyRaster(2, "
"2, 0, 0, 1, -1, 0, 0, 0), 1, '16BUI', 1, 0), 2, '8BUI', 10, 0), 3, '32BUI'::"
"text, 100, 0) AS rast UNION ALL\n"
"    SELECT 2 AS rid, ST_AddBand(ST_AddBand(ST_AddBand(ST_MakeEmptyRaster(2, "
"2, 0, 1, 1, -1, 0, 0, 0), 1, '16BUI', 2, 0), 2, '8BUI', 20, 0), 3, '32BUI'::"
"text, 300, 0) AS rast\n"
")\n"
"SELECT\n"
"    ST_MapAlgebra(\n"
"        t1.rast, 2,\n"
"        t2.rast, 1,\n"
"        '([rast2] + [rast1.val]) / 2'\n"
"    ) AS rast\n"
"FROM foo t1\n"
"CROSS JOIN foo t2\n"
"WHERE t1.rid = 1\n"
"    AND t2.rid = 2;"
msgstr ""
"WITH foo AS (\n"
"    SELECT 1 AS rid, ST_AddBand(ST_AddBand(ST_AddBand(ST_MakeEmptyRaster(2, "
"2, 0, 0, 1, -1, 0, 0, 0), 1, '16BUI', 1, 0), 2, '8BUI', 10, 0), 3, '32BUI'::"
"text, 100, 0) AS rast UNION ALL\n"
"    SELECT 2 AS rid, ST_AddBand(ST_AddBand(ST_AddBand(ST_MakeEmptyRaster(2, "
"2, 0, 1, 1, -1, 0, 0, 0), 1, '16BUI', 2, 0), 2, '8BUI', 20, 0), 3, '32BUI'::"
"text, 300, 0) AS rast\n"
")\n"
"SELECT\n"
"    ST_MapAlgebra(\n"
"        t1.rast, 2,\n"
"        t2.rast, 1,\n"
"        '([rast2] + [rast1.val]) / 2'\n"
"    ) AS rast\n"
"FROM foo t1\n"
"CROSS JOIN foo t2\n"
"WHERE t1.rid = 1\n"
"    AND t2.rid = 2;"

#. Tag: para
#: reference_raster.xml:7887
#, no-c-format
msgid ", <xref linkend=\"RT_ST_Union\"/>, <xref linkend=\"RT_ST_MapAlgebra\"/>"
