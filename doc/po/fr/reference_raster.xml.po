"FROM dummy_rast CROSS JOIN generate_series(1, 3) AS i CROSS JOIN "
"generate_series(1,3) AS j\n"
"WHERE rid = 2;\n"
"\n"
" col | row | origval | reclassval | reclassval_include_nodata\n"
"-----+-----+---------+------------+---------------------------\n"
"   1 |   1 |      78 |          9 |                         9\n"
"   2 |   1 |      98 |         14 |                        14\n"
"   3 |   1 |     122 |            |                         0\n"
"   1 |   2 |      96 |         14 |                        14\n"
"   2 |   2 |     118 |            |                         0\n"
"   3 |   2 |     180 |            |                         0\n"
"   1 |   3 |      99 |         15 |                        15\n"
"   2 |   3 |     112 |            |                         0\n"
"   3 |   3 |     169 |            |                         0"

