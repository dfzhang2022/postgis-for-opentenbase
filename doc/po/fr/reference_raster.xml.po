"    JOIN foo t2\n"
"        ON t1.rid = t2.rid\n"
")\n"
"SELECT\n"
"    rid,\n"
"    (ST_Metadata(rast)),\n"
"    (ST_BandMetadata(rast, 1)),\n"
"    ST_Value(rast, 1, 1, 1)\n"
"FROM bar;"
