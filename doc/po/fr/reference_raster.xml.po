#, no-c-format
msgid ""
"SET postgis.gdal_enabled_drivers = 'ENABLE_ALL';\n"
"SET postgis.enable_outdb_rasters = True;\n"
"\n"
"WITH apple AS (\n"
"    SELECT ST_AddBand(\n"
