#. Tag: para
#: reference_raster.xml:6039
#, no-c-format
msgid ""
"Returns a set of records with columns <varname>value</varname> "
"<varname>count</varname> which contain the pixel band value and count of "
"pixels in the raster tile or raster coverage of selected band."
msgstr ""
"Retourne un ensemble d'enregistrements contenant les colonnes "
"<varname>value</varname> <varname>count</varname>, correspondant à la valeur "
"de pixel dans la bande et le nombre de pixels ayant cette valeur dans la "
"bande du raster spécifié (ou de la couverture raster)."

#. Tag: para
#: reference_raster.xml:6040
#, no-c-format
msgid ""
"If no band is specified <varname>nband</varname> defaults to 1. If no "
"<varname>searchvalues</varname> are specified, will return all pixel values "
"found in the raster or raster coverage. If one searchvalue is given, will "
"return an integer instead of records denoting the count of pixels having "
"that pixel band value"
msgstr ""
"Si aucune bande <varname>nband</varname> n'est spécifiée, la bande 1 est "
"utilisée. Si aucune valeur <varname>searchvalues</varname> n'est spécifiée, "
"retourne toutes les valeurs de pixel trouvées dans le raster ou dans la "
"couverture raster. Si une valeur searchvalue est spécifiée, retourne un "
"entier au lieu d'enregistrements, correspondant au nombre de pixels ayant la "
"valeur recherchée"

#. Tag: programlisting
#: reference_raster.xml:6048
#, no-c-format
msgid ""
"UPDATE dummy_rast SET rast = ST_SetBandNoDataValue(rast,249) WHERE rid=2;\n"
"--Example will count only pixels of band 1 that are not 249. --\n"
"\n"
"SELECT (pvc).*\n"
"FROM (SELECT ST_ValueCount(rast) As pvc\n"
"    FROM dummy_rast WHERE rid=2) As foo\n"
"    ORDER BY (pvc).value;\n"
"\n"
" value | count\n"
"-------+-------\n"
"   250 |     2\n"
"   251 |     1\n"
"   252 |     2\n"
"   253 |     6\n"
"   254 |    12\n"
"\n"
"-- Example will coount all pixels of band 1 including 249 --\n"
"SELECT (pvc).*\n"
"FROM (SELECT ST_ValueCount(rast,1,false) As pvc\n"
"    FROM dummy_rast WHERE rid=2) As foo\n"
"    ORDER BY (pvc).value;\n"
"\n"
" value | count\n"
"-------+-------\n"
"   249 |     2\n"
"   250 |     2\n"
"   251 |     1\n"
"   252 |     2\n"
"   253 |     6\n"
"   254 |    12\n"
"\n"
"-- Example will count only non-nodata value pixels of band 2\n"
"SELECT (pvc).*\n"
"FROM (SELECT ST_ValueCount(rast,2) As pvc\n"
"    FROM dummy_rast WHERE rid=2) As foo\n"
"    ORDER BY (pvc).value;\n"
" value | count\n"
"-------+-------\n"
"    78 |     1\n"
"    79 |     1\n"
"    88 |     1\n"
"    89 |     1\n"
"    96 |     1\n"
"    97 |     1\n"
"    98 |     1\n"
"    99 |     2\n"
"   112 |     2\n"
":"
msgstr ""
"UPDATE dummy_rast SET rast = ST_SetBandNoDataValue(rast,249) WHERE rid=2;\n"
"--Exemple qui compte seulement les pixels de la bande 1 qui n'ont pas la "
"valeur 249. --\n"
"\n"
"SELECT (pvc).*\n"
"FROM (SELECT ST_ValueCount(rast) As pvc\n"
"    FROM dummy_rast WHERE rid=2) As foo\n"
"    ORDER BY (pvc).value;\n"
"\n"
" value | count\n"
"-------+-------\n"
"   250 |     2\n"
"   251 |     1\n"
"   252 |     2\n"
"   253 |     6\n"
"   254 |    12\n"
"\n"
"--Exemple qui compte tous les pixels de la bande 1, y compris ceux de valeur "
"249. --\n"
"SELECT (pvc).*\n"
"FROM (SELECT ST_ValueCount(rast,1,false) As pvc\n"
"    FROM dummy_rast WHERE rid=2) As foo\n"
"    ORDER BY (pvc).value;\n"
"\n"
" value | count\n"
"-------+-------\n"
"   249 |     2\n"
"   250 |     2\n"
"   251 |     1\n"
"   252 |     2\n"
"   253 |     6\n"
"   254 |    12\n"
"\n"
"--Exemple qui compte seulement les pixels differents de nodata de la bande "
"2\n"
"SELECT (pvc).*\n"
"FROM (SELECT ST_ValueCount(rast,2) As pvc\n"
"    FROM dummy_rast WHERE rid=2) As foo\n"
"    ORDER BY (pvc).value;\n"
" value | count\n"
"-------+-------\n"
"    78 |     1\n"
"    79 |     1\n"
"    88 |     1\n"
"    89 |     1\n"
"    96 |     1\n"
"    97 |     1\n"
"    98 |     1\n"
"    99 |     2\n"
"   112 |     2\n"
":"

#. Tag: programlisting
#: reference_raster.xml:6049
#, no-c-format
msgid ""
"--real live example.  Count all the pixels in an aerial raster tile band 2 "
"intersecting a geometry\n"
"-- and return only the pixel band values that have a count > 500\n"
"SELECT (pvc).value, SUM((pvc).count) As total\n"
"FROM (SELECT ST_ValueCount(rast,2) As pvc\n"
"    FROM o_4_boston\n"
"        WHERE ST_Intersects(rast,\n"
"            ST_GeomFromText('POLYGON((224486 892151,224486 892200,224706 "
"892200,224706 892151,224486 892151))',26986)\n"
"             )\n"
"        ) As foo\n"
"    GROUP BY (pvc).value\n"
"    HAVING SUM((pvc).count) > 500\n"
"    ORDER BY (pvc).value;\n"
"\n"
" value | total\n"
"-------+-----\n"
"    51 | 502\n"
"    54 | 521"
msgstr ""
"-- exemple plus reel. Compter tous les pixels de la bande 2 d'une tuile "
"raster aerienne intersectant une geometrie\n"
"-- et retourne seulement les valeurs qui ont un nombre de pixels > 500\n"
"SELECT (pvc).value, SUM((pvc).count) As total\n"
"FROM (SELECT ST_ValueCount(rast,2) As pvc\n"
"    FROM o_4_boston\n"
"        WHERE ST_Intersects(rast,\n"
"            ST_GeomFromText('POLYGON((224486 892151,224486 892200,224706 "
"892200,224706 892151,224486 892151))',26986)\n"
"             )\n"
"        ) As foo\n"
"    GROUP BY (pvc).value\n"
"    HAVING SUM((pvc).count) > 500\n"
"    ORDER BY (pvc).value;\n"
"\n"
" value | total\n"
"-------+-----\n"
"    51 | 502\n"
"    54 | 521"

#. Tag: programlisting
#: reference_raster.xml:6051
#, no-c-format
msgid ""
"-- Just return count of pixels in each raster tile that have value of 100 of "
"tiles that intersect  a specific geometry --\n"
"SELECT rid, ST_ValueCount(rast,2,100) As count\n"
"    FROM o_4_boston\n"
"        WHERE ST_Intersects(rast,\n"
"            ST_GeomFromText('POLYGON((224486 892151,224486 892200,224706 "
"892200,224706 892151,224486 892151))',26986)\n"
"             ) ;\n"
"\n"
" rid | count\n"
"-----+-------\n"
"   1 |    56\n"
"   2 |    95\n"
"  14 |    37\n"
"  15 |    64"
msgstr ""
"-- Retourne simplement le nombre de pixels de chaque tuile raster qui ont "
"une valeur de 100 et qui intersectent une geometrique en particulier --\n"
"SELECT rid, ST_ValueCount(rast,2,100) As count\n"
"    FROM o_4_boston\n"
"        WHERE ST_Intersects(rast,\n"
"            ST_GeomFromText('POLYGON((224486 892151,224486 892200,224706 "
"892200,224706 892151,224486 892151))',26986)\n"
"             ) ;\n"
"\n"
" rid | count\n"
"-----+-------\n"
"   1 |    56\n"
"   2 |    95\n"
"  14 |    37\n"
"  15 |    64"

