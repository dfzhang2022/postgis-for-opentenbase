"        ST_AddBand(\n"
"            ST_AddBand(\n"
"                ST_MakeEmptyRaster(10, 10, 0, 0, 1, -1, 0, 0, 0)\n"
"                , 1, '8BUI', 1, 0\n"
"            )\n"
"            , 2, '32BF', 0, -9999\n"
"        )\n"
"        , 3, '16BSI', 0, NULL\n"
