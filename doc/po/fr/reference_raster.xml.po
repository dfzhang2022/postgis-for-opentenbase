"WHERE rid = 2;\n"
"\n"
"col | row | ov1 | rv1 | ov2 | rv2 | ov3 | rv3\n"
"----+-----+-----+-----+-----+-----+-----+-----\n"
"  1 |   1 | 253 |   1 |  78 |   9 |  70 |   1\n"
"  2 |   1 | 254 |   0 |  98 |  14 |  86 |   3\n"
"  3 |   1 | 253 |   1 | 122 |   0 | 100 |   3\n"
